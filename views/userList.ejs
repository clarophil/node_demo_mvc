<html>
<head>
    <title>List Users</title>
    <link rel="stylesheet" href="/style.css">
    <meta charset='utf-8'>
</head>
<body>
    <a href="/user/add">Ajouter un nom</a>
    <h1>Liste des utilisateurs</h1>
    <ul>
        <% for (let i = 0 ;  i < users.length ; i++) { %>
        <li><%= users[i].lastname%> <%= users[i].firstname %> 
            <a href="/user/update/<%= i %>">Modifier</a> 
            <a href="/user/delete/<%= i %>">Effacer</a>
        </li> 
        <% } %>
    </ul>
</body>